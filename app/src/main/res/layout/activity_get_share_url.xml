<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    tools:context=".GetShareUrlActivity">

    <Button
        android:id="@+id/btn_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="位置Url" />

    <Button
        android:id="@+id/btn_nav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="导航Url" />

    <Button
        android:id="@+id/btn_poi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="POI_Url" />

    <Button
        android:id="@+id/btn_route_path"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="路径规划_驾车_Url" />

    <Button
        android:id="@+id/btn_route_bus_path"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="路径规划_公交_Url" />

    <Button
        android:id="@+id/btn_route_walk_path"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="路径规划_步行_Url" />

    <Button
        android:id="@+id/btn_get_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取拼接路径" />

    <TextView
        android:id="@+id/tv_show_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textIsSelectable="true" />
</LinearLayout>